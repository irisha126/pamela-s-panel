<h3>Let's create a new cohort</h3>


<%= form_for [@course, @cohort] do |f| %>

    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :start_date %>
        <%= f.date_field :start_date %>
    </div>
     <div>
        <%= f.label :end_date %>
        <%= f.date_field :end_date %>
    </div>
   
    <div>
        <%= f.submit %>
    </div>
    

<% end %>